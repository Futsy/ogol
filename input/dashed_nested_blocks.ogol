to squareDash :n :len
   if :n < 5 [
	   to dash :n :len
	      repeat :n [ pd; fd :len; pu; fd :len; 
	      	to deeplyNestedFun :n
	      		fd :n;
	      	end
	      	deeplyNestedFun :len;
	      ]
	      bk :len;
	      pd ;
	   end
	   repeat 4 [ dash :n :len; rt 90; ]
   ]
   to hi :n
   end
   
   hi :len;
end